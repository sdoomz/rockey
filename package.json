{
  "version": "1.0.0",
  "description":
    "Stressless CSS for components using JS. Write Component Based CSS with functional mixins.",
  "main": "index.js",
  "scripts": {
    "eslint-rockey": "esw --watch packages/rockey/lib",
    "eslint-rockey-react": "esw --watch packages/rockey-react/lib",
    "eslint-rockey-css-parse": "esw --watch packages/rockey-css-parse/lib",
    "test": "jest --coverage",
    "test-dev": "jest --watch",
    "build":
      "cross-env NODE_ENV=production babel -d ./build/rockey ./pakcages/rockey/",
    "format":
      "prettier --trailing-comma es5 --single-quote --write 'packages/*/lib/**/*.js' 'packages/*/!(node_modules)/**/*.js'",
    "optimize-parse": "node ./packages/rockey-css-parse/tasks/optimizeParse.js",
    "bench:nested":
      "cross-env NODE_ENV=development babel-node ./packages/rockey-css-parse/tasks/benchmark-nested.js",
    "bench:native":
      "cross-env NODE_ENV=development babel-node ./packages/rockey-css-parse/tasks/benchmark-native.js",
    "dev": "node ./tasks/dev",
    "clean": "node ./tasks/clean",
    "minify": "node ./tasks/minify",
    "precommit": "lint-staged",
    "transpile": "node ./tasks/transpile",
    "flow": "flow",
    "bootstrap": "lerna bootstrap --npm-client yarn"
  },
  "keywords": ["react", "css", "css-in-js", "rockey"],
  "author":
    "Valerii Sorokobatko <valeriy.sorokobatko@gmail.com> (https://github.com/tuchk4)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tuchk4/rockey/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tuchk4/rockey.git"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "chalk": "^2.3.1",
    "cross-env": "^5.1.3",
    "cross-spawn": "^6.0.4",
    "eslint": "^4.18.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.12.2",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lerna": "^2.9.0",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "spawn-sync": "^1.0.15",
    "webpack": "^4.0.1",
    "yargs": "^11.0.0"
  },
  "lint-staged": {
    "*.js": ["prettier --trailing-comma es5 --single-quote --write", "git add"]
  }
}
